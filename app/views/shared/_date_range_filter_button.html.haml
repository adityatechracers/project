.btn-group.pull-left{:style => "margin-left:10px;"}
  = link_to "#", :class => "btn btn-default #{@cdr_value.present? ? 'active':''}", :id => "cdr-calendar-btn" do
    %i.fa.fa-calendar{:style => "position:relative;top:1px;left:-3px;"}
    %span#cdr_value(data-default="#{t('shared.filter_by_date')}")
      = @cdr_value || t('shared.filter_by_date')
  %a.btn.btn-default#custom_date_range_filter
    %span.caret

